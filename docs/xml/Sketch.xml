<?xml version='1.0' encoding='utf-8'?>
<class id="Sketch" interface="true" name="Sketch" xmlns="http://jessevdk.github.com/cldoc/1.0">
  <brief> Abstract Class for Sketch</brief>
  <doc>This class defines several common
interfaces shared by all Sketches
in this library.
</doc>
  <subclass access="public" name="CountMin" ref="CountMin#CountMin" />
  <method abstract="yes" id="Sketch::processItem" name="processItem" virtual="yes">
    <return>
      <type builtin="yes" name="void" />
    </return>
    <argument id="Sketch::processItem::item" name="item">
      <type name="ItemType" qualifier=" const &amp;" ref="index#ItemType" />
    </argument>
    <argument id="Sketch::processItem::weight" name="weight">
      <type builtin="yes" name="double" />
    </argument>
  </method>
</class>
